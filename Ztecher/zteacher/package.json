{
  "name": "zteacher",
  "version": "1.0.0",
  "description": "zteacher package.json file",
  "main": "index.html",
  "scripts": {
    "start": "npm run watch:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lite": "lite-server",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange \"css/*.scss\" -- npm run scss",
    "watch:all": "parallelshell \"npm run watch:scss\" \"npm run lite\"",
    "clean": "rimraf dist",
    "copyfonts": "copyfiles -f node_modules/font-awesome/fonts/* dist/fonts",
    "imagemin": "imagemin img/* --out-dir=\"dist/img\"",
    "usemin": "usemin contactus.html -d dist --htmlmin -o dist/contactus.html && usemin aboutus.html -d dist --htmlmin -o dist/aboutus.html && usemin index.html -d dist --htmlmin -o dist/index.html",
    "build": "npm run clean && npm run imagemin && npm run copyfonts && npm run usemin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://Dima_Shutenko@bitbucket.org/Dima_Shutenko/zteacher.git"
  },
  "author": "Dima Shutenko",
  "license": "ISC",
  "homepage": "https://bitbucket.org/Dima_Shutenko/zteacher#readme",
  "devDependencies": {
    "browser-sync": "^2.26.12",
    "cssmin": "^0.4.3",
    "del": "^6.0.0",
    "event-stream": "^4.0.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-changed": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-flatmap": "^1.0.2",
    "gulp-htmlmin": "^5.0.1",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^7.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-rev": "^9.0.0",
    "gulp-rev-append": "^0.1.8",
    "gulp-rev-collector": "^1.3.3",
    "gulp-sass": "^4.1.0",
    "gulp-uglify": "^3.0.2",
    "gulp-uglify-es": "^2.0.0",
    "gulp-uglifycss": "^1.1.0",
    "gulp-usemin": "^0.3.30",
    "gulp-useref": "^4.0.1",
    "htmlmin": "0.0.7",
    "lite-server": "^2.5.4",
    "node-sass": "^4.14.1",
    "onchange": "^7.0.2",
    "parallelshell": "^3.0.1",
    "rimraf": "^2.6.2",
    "uglify-js": "^3.11.0",
    "usemin-cli": "^0.6.0"
  },
  "dependencies": {
    "bootstrap": "^4.0.0",
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0",
    "gulp-cssnano": "^2.1.3",
    "jquery": "^3.3.1",
    "popper.js": "^1.12.9",
    "useref": "^1.4.3"
  }
}
